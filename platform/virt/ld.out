ENTRY(_start)
SECTIONS
{
	. = 0xC0000000;
	._reset . : { ../../.objs/reset.o(.text) }
	.text : ALIGN(8) {
		*(.text)
		*(.vectors)
	}
	.data : ALIGN(8) { *(.data) }
	.bss : ALIGN(16) { *(.bss COMMON) }
	xlat_table (NOLOAD) : ALIGN(16) {
		__XLAT_START__ = .;
		*(xlat_table)
		__XLAT_END__ = .;
	}
	. = ALIGN(8);
}
